<div id="admin" class="card">
  <h1 class="title"><%= @admin.name %></h1>
  <div class="row">
    <div class="column">
      <table class="table">  
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Created at</th>
          <th>Updated at</th>
        </tr>
        <tr>
          <td><%= @admin.name %></td>
          <td><%= @admin.email %></td>
          <td><%= @admin.created_at %></td>
          <td><%= @admin.updated_at %></td>
        </tr>
      </table>
    </div>
  </div>
</div>