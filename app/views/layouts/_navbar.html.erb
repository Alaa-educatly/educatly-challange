<div class="top-bar">
    <% if logged_in?%>
        <span> <%= link_to "Products", products_path, class: 'menu-item' %> </span>
        <span> <%= link_to "Log out", logout_path,  data: {"turbo-method": :delete }, class: 'menu-item' %> </span>
    <% else%>
        <span> <%= link_to "Log in", login_path, class: 'menu-item' %> </span>
        <span> <%= link_to "Create Admin", new_admin_path, class: 'menu-item' %> </span>
    <% end%>
</div>